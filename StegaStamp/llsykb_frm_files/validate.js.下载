	//_path:路径 ，id：选择值的ID ，Name：选择值的文本内容,type:0为多选 1为单选 ，where：查询条件
	function selectKcxx_Yx(_path,id,name,yx,kcsx,zxs,kcbh,xf,khfs,kslb,kcxzm,szkcfl,type,where) {
	 	if(type == undefined) type="0";
	 	if(where == undefined) where="";
